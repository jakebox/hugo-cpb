html{height:100%}body{font-family:system-ui,-apple-system,BlinkMacSystemFont,segoe ui,helvetica neue,helvetica,arial,sans-serif;font-size:18px;line-height:1.7;background-color:#fdfdfd;color:rgba(0,0,0,.8);margin:0;box-sizing:border-box}@media only screen and (min-width:768px){body{height:100%;overflow:hidden;display:flex}}a{color:#000}a:visited{text-decoration:none;color:#000}a:hover{text-decoration:none;color:blue}h1,h2,h3,h4,h5,h6{font-family:georgia,serif;font-weight:400}h1{font-size:36px}h2{font-size:30px}h3{font-size:24px}h4{font-size:20px}h5,h6{text-transform:uppercase;font-size:1rem}main h1,main h2,main h3{margin:1.5em 0 .5em}main h4{margin:2em 0 .5em}main h5,main h6{margin:2em 0 .5em}h1>a.anchor::before,h2>a.anchor::before,h3>a.anchor::before,h4>a.anchor::before,h5>a.anchor::before,h6>a.anchor::before{content:"\00A7";font-size:.85em;display:block;padding-top:.15em}h1>a.anchor,h2>a.anchor,h3>a.anchor,h4>a.anchor,h5>a.anchor,h6>a.anchor{position:absolute;z-index:1001;width:1.5ex;margin-left:-1.5ex;display:block;text-decoration:none;color:inherit;visibility:hidden;text-align:center}h1:hover>a.anchor,h2:hover>a.anchor,h3:hover>a.anchor,h4:hover>a.anchor,h5:hover>a.anchor,h6:hover>a.anchor{visibility:visible}h1.title{font-weight:700;margin:.5em 0}.column{height:100%;display:flex;flex-direction:column}#left{background-color:#f5f5f5;flex-shrink:0;min-width:300px;overflow-y:auto;font-size:16px}#right{flex-grow:1;overflow-y:auto}nav.menu{padding:1em}nav.menu ul li.active{font-weight:600}main{padding:1em;position:relative}main .content{max-width:48rem;margin-left:auto;margin-right:auto}main .admin-container{position:absolute;top:0;right:0;margin:.6em 1.6em;font-size:.8rem;color:#999}main .admin-container a:link,main .admin-container a:visited,main .admin-container a:active{color:#999}main .admin-container a:hover{color:#888}.toc #table-of-contents{font-family:noto serif,georgia,serif;font-weight:300;font-size:1.4em}.toc nav{border-bottom:1px solid #e7e7e9}.toc nav>ul{margin-top:.2em}div.highlight pre{background-color:#f7f7f8!important}pre{border:1px solid #ddd;padding:.5em 1em;overflow-x:auto;background-color:#f7f7f8}pre code{background:0 0;padding:0;font-size:.8em}code{background:#e0e0e0;font-family:lucida console,Monaco,monospace;font-size:.8em;padding:.2em .4em}hr{margin:2em 0;border:0;height:1px;background:#ccc}p{line-height:1.6}img,iframe,video{max-width:100%}blockquote{background:#f7f7f8;border-left:5px solid #ccc;padding:3px 1em}table{border-collapse:collapse;margin:auto}table thead th{text-align:left;border-bottom:1px solid #ccc}table th,table td{padding:5px}table thead th{border-bottom:1px solid #ddd}th,td{padding:5px}thead,tfoot,tr:nth-child(even){background:#eee}.footnotes ol li p{margin-top:0;margin-bottom:0}details>summary{cursor:pointer}